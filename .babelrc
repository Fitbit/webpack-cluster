{
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "presets": [
    "es2015"
  ],
  "plugins": [
    ["babel-plugin-transform-builtin-extend", {
      "globals": [
        "Error"
      ]
    }],
    "add-module-exports",
    "transform-runtime"
  ],
  "ignore": [
    "node_modules/",
    "**/wallaby/wallaby/**"
  ]
}
